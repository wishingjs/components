var nanoid = function nanoid() {
  if (typeof window === 'undefined') return genNanoid();
  // @ts-ignore
  if (window.crypto && window.crypto.randomUUID && typeof crypto.randomUUID == 'function') {
    // @ts-ignore
    return crypto.randomUUID();
  }
  return genNanoid();
};
