function mergeProps(origin, target) {
  var clone = objectSpread2_objectSpread2({}, origin);
  Object.keys(target).forEach(function (key) {
    var value = target[key];
    if (value !== undefined) {
      clone[key] = value;
    }
  });
  return clone;
}
