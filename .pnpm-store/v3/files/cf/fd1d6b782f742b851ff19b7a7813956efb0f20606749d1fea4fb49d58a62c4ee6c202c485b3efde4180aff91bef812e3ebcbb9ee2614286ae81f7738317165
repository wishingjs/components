var SPLIT = '__@field_split__';
/**
 * Convert name path into string to fast the fetch speed of Map.
 */
function NameMap_normalize(namePath) {
  return namePath.map(function (cell) {
    return "".concat(typeof_typeof(cell), ":").concat(cell);
  })
  // Magic split
  .join(SPLIT);
}
