      measure.registerPath(eventKey, connectedKeyPath);
      return function () {
        measure.unregisterPath(eventKey, connectedKeyPath);
      };
    }
  }, [connectedKeyPath]);
  if (measure) {
    return null;
  } // ======================== Render ========================

  return /*#__PURE__*/external_React_.createElement(InternalMenuItem, props);
}
