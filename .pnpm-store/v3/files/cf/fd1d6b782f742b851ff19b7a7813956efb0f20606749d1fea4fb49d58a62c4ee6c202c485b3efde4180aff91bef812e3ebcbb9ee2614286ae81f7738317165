;// CONCATENATED MODULE: ./packages/form/es/components/SchemaForm/valueType/ignore.js
var ignore = function ignore(item) {
  // 几种特殊的 value 不处理
  if (item.valueType && typeof item.valueType === 'string' && ['index', 'indexBorder', 'option'].includes(item === null || item === void 0 ? void 0 : item.valueType)) {
    return null;
  }
  return true;
};
;// CONCATENATED MODULE: ./packages/form/es/components/SchemaForm/valueType/index.js







// 按照数组顺序执行
var tasks = [ignore, group, formList, formSet, divider, dependency];
var renderValueType = function renderValueType(item, helpers) {
  for (var cur = 0; cur < tasks.length; cur++) {
    var task = tasks[cur];
    var dom = task(item, helpers);
    // False 不再遍历
    // if (dom === false) {
    //   return false;
    if (dom === true) {
      // True 继续下一次
      continue;
    } else {
      // Other Is Dom
      return dom;
    }
  }
  // 最后执行
  return field(item, helpers);
};
