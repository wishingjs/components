  return refAble ? /*#__PURE__*/external_React_.memo( /*#__PURE__*/external_React_.forwardRef(ImmutableComponent), propsAreEqual) : /*#__PURE__*/external_React_.memo(ImmutableComponent, propsAreEqual);
}
