var formatDate = function formatDate(text, format) {
  if (!text) {
    return '-';
  }
  if (typeof format === 'function') {
    return format(dayjs_min_default()(text));
  } else {
    return dayjs_min_default()(text).format(format || 'YYYY-MM-DD');
  }
