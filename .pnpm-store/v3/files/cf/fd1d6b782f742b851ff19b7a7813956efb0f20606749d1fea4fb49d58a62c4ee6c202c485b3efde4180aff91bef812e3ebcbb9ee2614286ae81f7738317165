;// CONCATENATED MODULE: ./packages/utils/es/runFunction/index.js
/** 如果是个方法执行一下它 */
function runFunction(valueEnum) {
  if (typeof valueEnum === 'function') {
    for (var _len = arguments.length, rest = new Array(_len > 1 ? _len - 1 : 0), _key = 1; _key < _len; _key++) {
      rest[_key - 1] = arguments[_key];
    }
    return valueEnum.apply(void 0, rest);
  }
  return valueEnum;
}
;// CONCATENATED MODULE: ./packages/utils/es/hooks/useRefFunction/index.js

var useRefFunction = function useRefFunction(reFunction) {
