    }
    return (_ref$current = ref.current) === null || _ref$current === void 0 ? void 0 : _ref$current.call.apply(_ref$current, [ref].concat(rest));
  }, []);
};

;// CONCATENATED MODULE: ./packages/utils/es/hooks/usePrevious/index.js

var usePrevious = function usePrevious(state) {
  var ref = (0,external_React_.useRef)();
  (0,external_React_.useEffect)(function () {
    ref.current = state;
  });
  return ref.current;
};
;// CONCATENATED MODULE: ./packages/utils/es/isDeepEqualReact/index.js


/* eslint-disable no-restricted-syntax */
/* eslint-disable no-continue */
/* eslint-disable @typescript-eslint/no-unused-vars */
/* eslint-disable no-self-compare */
